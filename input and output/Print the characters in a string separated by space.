var str="Prajwal";
var char_arr=str.split(""); // strin converstion to array
var otput=char_arr.join(" "); // converstion of  char array to space sepreated string
console.log((otput));
var otput=char_arr.join(""); // converstion of  char array to space no-space string
console.log((otput));
var otput=char_arr.join(","); // converstion of  char array to space comma string
console.log((otput));
